{
  "name": "smd-desktop",
  "version": "1.0.0",
  "description": "The best desktop app to download Spotity songs for free and legally !!",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . build SMD Desktop --no-prune",
    "build-all": "electron-packager . build SMD Desktop --all --no-prune",
    "package": "npm start && npm build",
    "package-all": "npm start && npm build-all",
    "test": "echo \"Error: no test specified\" && exit 1"
  }, 
  "repository": {
    "type": "git",
    "url": "www.github.com/noahweasley/SMD-desktop"
  },
  "keywords": [
    "javascript",
    "electron",
    "music",
    "spotify",
    "spotify-downloader"
  ],
  "author": "NoahWeasley",
  "license": "ISC",
  "devDependencies": {
    "electron": "^15.3.1",
    "electron-packager": "^15.4.0"
  },
  "dependencies": {
    "spotify-web-api-node": "^5.0.2"
  }
}
